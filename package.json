{
    "name": "referenceshelper",
    "displayName": "Find all references",
    "description": "Find all references for almost all files.",
    "repository": {
        "type": "git",
        "url": "https://github.com/gayanhewa/vscode-find-all-references"
    },
    "version": "0.2.3",
    "publisher": "gayanhewa",
    "author": "Gayan Hewa <gayanhewa@gmail.com>",
    "license": "MIT",
    "engines": {
        "vscode": "^1.13.0"
    },
    "keywords": [
        "find occurences",
        "find references",
        "navigation",
        "php",
        "ruby",
        "rust",
        "python",
        "erlang",
        "java",
        "cpp",
        "c",
        "markdown",
        "xml",
        "haml",
        "twig",
        "bat",
        "bibtex",
        "clojure",
        "coffeescript",
        "css",
        "csharp",
        "diff",
        "dockerfile",
        "fsharp",
        "go",
        "groovy",
        "handlebars",
        "html",
        "ini",
        "json",
        "latex",
        "less",
        "lua",
        "makefile",
        "objective-c",
        "objective-cpp",
        "perl6",
        "perl",
        "powershell",
        "jade",
        "python",
        "r",
        "razor",
        "scss",
        "shaderlab",
        "shellscript",
        "sql",
        "swift",
        "tex",
        "xsl",
        "yaml",
        "intellisense"
    ],
    "icon": "assets/icon.png",
    "categories": [
        "Languages",
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "properties": {
                "find.all.references.engine": {
                    "default": "rg"
                },
                "find.all.references.options": {
                    "default": ""
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
